{
	"private": true,
	"packageManager": "pnpm@6.32.3",
	"engines": {
		"node": "18.15.0"
	},
	"scripts": {
		"build": "vite build",
		"dev": "concurrently \"pnpm generate-gql-watch\" \"vite --port 4000\"",
		"lint": "eslint .",
		"preview": "vite preview",
		"preview-https": "serve dist",
		"typecheck": "vue-tsc --noEmit",
		"storybook": "set NODE_OPTIONS=--openssl-legacy-provider&& start-storybook -p 6006",
		"generate-gql": "graphql-codegen --require dotenv/config --config codegen.yml",
		"generate-gql-watch": "graphql-codegen --require dotenv/config --config codegen.yml --watch"
	},
